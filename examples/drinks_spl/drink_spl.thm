   <type name="Return"/>
   <type name="S"/>
   <type name="S1"/>
   <type name="S2"/>
   <type name="S3"/>
   <type name="S4"/>
   <type name="S5"/>
   <type name="S6"/>
   <type name="S7"/>
   <type name="S8"/>
   <type name="ServeSoda"/>
   <type name="ServeTea"/>
