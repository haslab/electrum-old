   <set name="_this/Tea" type="Tea"/>
</node>

<node visible="no">
   <type name="Change"/>
   <type name="Event"/>
   <type name="Feature"/>
   <type name="State"/>
</node>

<node visible="yes" showlabel="no" color="Blue">
   <set name="_this/Current" type="State"/>
</node>

<node visible="yes" showlabel="no" color="Green">
   <set name="_this/Change" type="Change"/>
   <set name="_this/Event" type="Event"/>
</node>

<node visible="yes" showlabel="no" color="Red">
   <set name="this/Product" type="Feature"/>
</node>

</view>

</alloy>
